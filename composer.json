{
    "name": "laraish/laraish",
    "description": "The WordPress theme powered by the Laravel Framework.",
    "keywords": [
        "framework",
        "laravel",
        "WordPress"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "amrshawky/laravel-currency": "dev-master",
        "ccxt/ccxt": "^1.50",
        "guzzlehttp/guzzle": "^7.0.1",
        "hautelook/phpass": "^0.3.6",
        "htmlburger/carbon-fields": "^3.3",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "johnbillion/extended-cpts": "^4.5",
        "jwilsson/spotify-web-api-php": "^5.5",
        "laraish/contracts": "*",
        "laraish/framework": "*",
        "laraish/options": "*",
        "laraish/pagination": "*",
        "laraish/support": "*",
        "laravel/horizon": "^5.10",
        "laravel/socialite": "^5.1",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "^4.3",
        "livewire/livewire": "^2.10",
        "mll-lab/laravel-graphiql": "^3.1",
        "nuwave/lighthouse": "^6.13",
        "peerme/mx-sdk-laravel": "dev-patch-1",
        "peerme/mx-sdk-php": "dev-patch-1 as 1.0.2",
        "psr/log": "^3.0",
        "socialiteproviders/spotify": "^4.1",
        "spatie/browsershot": "^3.41",
        "spatie/ray": "^1.1",
        "stoutlogic/acf-builder": "^1.10",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "laraish/*"
        },
        {
            "type": "vcs",
            "url": "https://github.com/thanasisxan/laraish-contracts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/thanasisxan/laraish-support"
        },
        {
            "type": "vcs",
            "url": "https://github.com/thanasisxan/laraish-pagination"
        },
        {
            "type": "vcs",
            "url": "https://github.com/thanasisxan/laraish-options"
        },
        {
            "type": "vcs",
            "url": "https://github.com/parth391/laravel-currency"
        },
        {
            "type": "vcs",
            "url": "https://github.com/filipac/mx-sdk-laravel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/filipac/mx-sdk-php/"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "Laraish\\Foundation\\ComposerScripts::renameHelperFunctions",
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Laraish\\Foundation\\ComposerScripts::renameHelperFunctions"
        ],
        "post-autoload-dump": [
            "Laraish\\Foundation\\ComposerScripts::renameHelperFunctions",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
